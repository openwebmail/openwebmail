          </tmpl_unless>
        <tmpl_else>
          <tmpl_if timelabel>
            <tmpl_if is_timelabel_weekday>
              <nobr><a href="<tmpl_var url_cgi escape="none">/openwebmail-cal.pl?action=caledit&amp;eventid=&amp;year=<tmpl_var weekday_year>&amp;month=<tmpl_var weekday_month>&amp;day=<tmpl_var weekday_day>&amp;cal_caller=<tmpl_var cal_caller escape="url">&amp;sessionid=<tmpl_var sessionid escape="url">&amp;folder=<tmpl_var folder escape="url">&amp;sort=<tmpl_var sort escape="url">&amp;msgdatetype=<tmpl_var msgdatetype escape="url">&amp;page=<tmpl_var page escape="url">&amp;longpage=<tmpl_var longpage escape="url">&amp;searchtype=<tmpl_var searchtype escape="url">&amp;keyword=<tmpl_var keyword escape="url">&amp;message_id=<tmpl_var message_id escape="url">" title="gettext('add event')"><span class="smalltext <tmpl_if is_sunday>colortext<tmpl_else><tmpl_if is_saturday>quotedtext</tmpl_if></tmpl_if>">+</span></a><a href="<tmpl_var url_cgi escape="none">/openwebmail-cal.pl?action=calday&amp;year=<tmpl_var weekday_year>&amp;month=<tmpl_var weekday_month>&amp;day=<tmpl_var weekday_day>&amp;sessionid=<tmpl_var sessionid escape="url">&amp;folder=<tmpl_var folder escape="url">&amp;sort=<tmpl_var sort escape="url">&amp;msgdatetype=<tmpl_var msgdatetype escape="url">&amp;page=<tmpl_var page escape="url">&amp;longpage=<tmpl_var longpage escape="url">&amp;searchtype=<tmpl_var searchtype escape="url">&amp;keyword=<tmpl_var keyword escape="url">&amp;message_id=<tmpl_var message_id escape="url">" title="gettext('Dayview <tmpl_var weekday_monthname> <tmpl_var weekday_day> <tmpl_var weekday_year>')">
              <tmpl_if is_sunday>
              <span class="colortext"><tmpl_var timelabel escape="html"> <tmpl_var weekday_day></span>
              <tmpl_else>
                <tmpl_if is_saturday>
                <span class="quotedtext"><tmpl_var timelabel escape="html"> <tmpl_var weekday_day></span>
                <tmpl_else>
                <tmpl_var timelabel escape="html"> <tmpl_var weekday_day>
                </tmpl_if>
              </tmpl_if>
              </a></nobr>
              <tmpl_if weekday_lunar>
              <br><span class="<tmpl_if weekday_lunar_new>smalltext<tmpl_else>smalllighttext</tmpl_if>"><tmpl_var weekday_lunar escape="html">
              </tmpl_if>
            </tmpl_if>
            <tmpl_if is_timelabel_allday><tmpl_var timelabel escape="html"></tmpl_if>
            <tmpl_if is_timelabel_hour><tmpl_var timelabel escape="html"></tmpl_if>
            <tmpl_if is_timelabel_minute><span class="lighttext"><tmpl_var timelabel escape="html"></span></tmpl_if>
          <tmpl_else>
            <tmpl_if no_events>
              gettext('There are no events today')
            <tmpl_else>
              &nbsp;
            </tmpl_if>
          </tmpl_if>
        </tmpl_if>
      </td>
      </tmpl_unless>
      </tmpl_loop>
    </tr>
    </tmpl_loop>
    </table>
  </td>
</tr>
</table>

