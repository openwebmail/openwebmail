<tmpl_var header_template escape="none">

<tmpl_loop contactloop>
<table cellpadding="0" cellspacing="0" border="0" align="center" width="100%">
<tr>
  <td class="windowdark">
    <table cellpadding="0" cellspacing="4" border="0" width="100%">
    <tr>
      <td valign="top"><!-- left side -->
        <table cellpadding="0" cellspacing="0" border="0" width="100%">
        <tr>
          <tmpl_if PHOTO>
          <tmpl_loop PHOTO>
          <tmpl_if URI>
          <tmpl_if show_inline>
          <td width="%1"><a name="PHOTO" href="<tmpl_var VALUE escape="html">" target="_new"><img src="<tmpl_var VALUE escape="html">" border="1" height="50"></a></td>
          <tmpl_else>
          <td width="62" height="62" bgcolor="#000000" align="center">
            <table cellspacing="0" cellpadding="0" border="0">
            <tr>
              <td align="center" width="60" height="60" class="rowlight">
                <a name="PHOTO" href="<tmpl_var VALUE escape="html">" target="_new" class="icon"><tmpl_if use_texticon><tmpl_var VALUE escape="html"><tmpl_else><img src="<tmpl_var url_html escape="none">/images/iconsets/<tmpl_var iconset escape="html">/cal-link.gif" title="<tmpl_var VALUE escape="html">" alt="<tmpl_var VALUE escape="html">" border="0" align="absmiddle"></tmpl_if></a>
              </td>
            </tr>
            </table>
          </td>
          </tmpl_if>
          <tmpl_else>
          <tmpl_if BASE64>
          <td width="%1">
            <a name="PHOTO" href="<tmpl_var url_cgi escape="html">/openwebmail-abook.pl?action=addrviewatt&amp;sessionid=<tmpl_var sessionid escape="html">&amp;file=<tmpl_var VALUE escape="html">&amp;type=<tmpl_var filetype escape="html">" target="_new"><img src="<tmpl_var url_cgi escape="html">/openwebmail-abook.pl?action=addrviewatt&amp;sessionid=<tmpl_var sessionid escape="html">&amp;file=<tmpl_var VALUE escape="html">&amp;type=<tmpl_var filetype escape="html">" border="1" height="60"></a>
          </td>
          </tmpl_if>
          </tmpl_if>
          </tmpl_loop>
          <td>&nbsp;</td>
          </tmpl_if>

          <td valign="top">
            <table cellpadding="0" cellspacing="0" border="0" width="100%">
            <tr>
              <td width="100%">
                <table cellpadding="0" cellspacing="0" border="0" width="100%">
                <tr>
                  <tmpl_if FN>
                  <td>
                    <tmpl_loop FN>
                    <a href="<tmpl_var url_cgi escape="none">/openwebmail-abook.pl?action=addreditform&amp;rootxowmuid=<tmpl_var rootxowmuid escape="url">&amp;editformcaller=<tmpl_var editformcaller escape="url">&amp;sessionid=<tmpl_var sessionid escape="url">&amp;folder=<tmpl_var folder escape="url">&amp;sort=<tmpl_var sort escape="url">&amp;msgdatetype=<tmpl_var msgdatetype escape="url">&amp;page=<tmpl_var page escape="url">&amp;longpage=<tmpl_var longpage escape="url">&amp;searchtype=<tmpl_var searchtype escape="url">&amp;keyword=<tmpl_var keyword escape="url">&amp;message_id=<tmpl_var messageid escape="url">&amp;abookfolder=<tmpl_var abookfolder escape="url">&amp;abookpage=<tmpl_var abookpage escape="url">&amp;abooklongpage=<tmpl_var abooklongpage escape="url">&amp;abooksort=<tmpl_var abooksort escape="url">&amp;abooksearchtype=<tmpl_var abooksearchtype escape="url">&amp;abookkeyword=<tmpl_var abookkeyword escape="url">&amp;abookcollapse=<tmpl_var abookcollapse escape="url">" title="edit contact" target="_parent"><b><tmpl_var VALUE escape="html"></b></a>
                    </tmpl_loop>

                    <tmpl_loop SOUND>
                    <tmpl_if URI>
                    <tmpl_if is_phonetic>
                    <span class="smalltext">phonetic pronunciation:&nbsp;<tmpl_var VALUE escape="html"></span>
                    <tmpl_else>
                    <a name="SOUND" href="<tmpl_var VALUE escape="html">" target="_new"><tmpl_if use_texticon>play pronunciation sound<tmpl_else><img src="<tmpl_var url_html escape="none">/images/iconsets/<tmpl_var iconset escape="html">/abooksound.gif" title="play pronunciation sound" alt="play pronunciation sound" border="0" align="absmiddle"></tmpl_if></a>
                    </tmpl_if>
                    <tmpl_else>
                    <tmpl_if BASE64>
                    <a name="SOUND" href="<tmpl_var url_cgi escape="html">/openwebmail-abook.pl?action=addrviewatt&amp;sessionid=<tmpl_var sessionid escape="html">&amp;file=<tmpl_var VALUE escape="html">&amp;type=<tmpl_var filetype escape="html">" target="_new"><tmpl_if use_texticon>play pronunciation sound<tmpl_else><img src="<tmpl_var url_html escape="none">/images/iconsets/<tmpl_var iconset escape="html">/abooksound.gif" title="play pronunciation sound" alt="play pronunciation sound" border="0" align="absmiddle"></tmpl_if></a>
                    </tmpl_if>
                    </tmpl_if>
                    </tmpl_loop>
                  </td>
                  <tmpl_else>
                  <tmpl_if N>
                  <tmpl_loop N>
                  <td>
                    <a href="<tmpl_var url_cgi escape="none">/openwebmail-abook.pl?action=addreditform&amp;rootxowmuid=<tmpl_var rootxowmuid escape="url">&amp;editformcaller=<tmpl_var editformcaller escape="url">&amp;sessionid=<tmpl_var sessionid escape="url">&amp;folder=<tmpl_var folder escape="url">&amp;sort=<tmpl_var sort escape="url">&amp;msgdatetype=<tmpl_var msgdatetype escape="url">&amp;page=<tmpl_var page escape="url">&amp;longpage=<tmpl_var longpage escape="url">&amp;searchtype=<tmpl_var searchtype escape="url">&amp;keyword=<tmpl_var keyword escape="url">&amp;message_id=<tmpl_var messageid escape="url">&amp;abookfolder=<tmpl_var abookfolder escape="url">&amp;abookpage=<tmpl_var abookpage escape="url">&amp;abooklongpage=<tmpl_var abooklongpage escape="url">&amp;abooksort=<tmpl_var abooksort escape="url">&amp;abooksearchtype=<tmpl_var abooksearchtype escape="url">&amp;abookkeyword=<tmpl_var abookkeyword escape="url">&amp;abookcollapse=<tmpl_var abookcollapse escape="url">" title="edit contact" target="_parent"><b><tmpl_var NAMEPREFIX escape="html"> <tmpl_var GIVENNAME escape="html"> <tmpl_var ADDITIONALNAMES escape="html"> <tmpl_var FAMILYNAME escape="html"> <tmpl_var NAMESUFFIX escape="html"></b></a>
                  </td>
                  </tmpl_loop>
                  </tmpl_if>
                  </tmpl_if>

                  <tmpl_if BDAY>
                  <td class="smalltext" align="right" nowrap>
                  <tmpl_loop BDAY>
                  <tmpl_var MONTH escape="html">/<tmpl_var DAY escape="html">/<tmpl_var YEAR escape="html"> (<tmpl_var AGE escape="html">)<br>
                  </tmpl_loop>
                  </tmpl_if>
                  </td>
                </tr>
                </table>
              </td>
            </tr>

            <tmpl_if ORG>
            <tmpl_loop ORG>
            <tmpl_if ORGANIZATIONNAME>
            <tr>
              <td class="smalltext"><tmpl_if TITLE><tmpl_var TITLE escape="html">, </tmpl_if><tmpl_var ORGANIZATIONNAME escape="html"></td>
            </tr>
            </tmpl_if>
            </tmpl_loop>
            </tmpl_if>

            <tmpl_if EMAIL>
            <tr>
              <td nowrap>
                <form name="emailform">
                <tmpl_unless quotaoverlimit><a href="javascript:void(0);" onclick="javascript:parent.window.location='<tmpl_var url_cgi escape="none">/openwebmail-send.pl?action=compose&amp;composetype=sendto&amp;to=' + document.forms['emailform'].elements['email'].options[document.forms['emailform'].elements['email'].selectedIndex].value + '&amp;compose_caller=listmessages&amp;sessionid=<tmpl_var sessionid escape="url">&amp;folder=<tmpl_var folder escape="url">&amp;sort=<tmpl_var sort escape="url">&amp;msgdatetype=<tmpl_var msgdatetype escape="url">&amp;page=<tmpl_var page escape="url">&amp;longpage=<tmpl_var longpage escape="url">&amp;searchtype=<tmpl_var searchtype escape="url">&amp;keyword=<tmpl_var keyword escape="url">';" class="icon"><tmpl_if use_texticon><tmpl_var VALUE escape="html"><tmpl_else><img src="<tmpl_var url_html escape="none">/images/iconsets/<tmpl_var iconset escape="html">/editfroms.s.gif" title="compose message" alt="compose message" border="0" align="absmiddle"></tmpl_if></a></tmpl_unless>
                <select name="email">
                  <tmpl_loop EMAIL>
                  <option value="<tmpl_var nameaddress escape="html">" <tmpl_if selected>selected</tmpl_if>><tmpl_var VALUE escape="html"><tmpl_if PREF>&nbsp;*</tmpl_if></option>
                  </tmpl_loop>
                </select>
                </form>
              </td>
            </tr>
            </tmpl_if>
            </table>
          </td>
        </tr>
        </table>
      </td>
    </tr>

    <tmpl_if TEL>
    <tr>
      <td colspan="2"><!-- left and right side -->
        <table cellpadding="0" cellspacing="2" border="0">
        <tmpl_loop TEL>
        <tr>
          <td><tmpl_if HOME>home </tmpl_if><tmpl_if WORK>work </tmpl_if><tmpl_if CELL>cell </tmpl_if><tmpl_if CAR>car </tmpl_if><tmpl_if VIDEO>video </tmpl_if><tmpl_if PAGER>pager </tmpl_if><tmpl_if FAX>fax </tmpl_if><tmpl_if ISDN>ISDN </tmpl_if><tmpl_if BBS>BBS </tmpl_if><tmpl_if MODEM>modem </tmpl_if><tmpl_if MSG>message service </tmpl_if></td>
          <td>:</td>
          <td><tmpl_var VALUE escape="html"></td>
        </tr>
        </tmpl_loop>
        </table>
      </td>
    </tr>
    </tmpl_if>
    </table>
  </td>
</tr>
</table>
</tmpl_loop>

