               tr.className = 'rowlight';
            }
         }
      }
   }
}

function OpConfirm(text, conf) {
   for (var i=0;i<document.pageform.elements.length;i++) {
      var e = document.pageform.elements[i];
      if (e.type == 'checkbox' && e.name != 'allbox' && e.checked == 1 ) {
         if (conf) {
            return confirm(text);
         } else {
            return 1;
         }
      }
   }
   return false;
}

